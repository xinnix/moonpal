# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# LLM 配置 (当前选择: zhipu)
LLM_PROVIDER=zhipu           # 可选: zhipu | minimax | claude | doubao | qwen | spark
LLM_API_KEY=your-llm-api-key
LLM_ENDPOINT=https://open.bigmodel.cn/api/paas/v4/chat/completions
LLM_MODEL=glm-4.5-flash

# TTS 配置 (当前选择: minimax)
TTS_PROVIDER=minimax         # 可选: minimax | azure | openai | 火山 | 讯飞
TTS_API_KEY=your-tts-api-key
TTS_GROUP_ID=your-tts-group-id
TTS_ENDPOINT=https://api.minimax.io/v1/t2a_v2
TTS_MODEL=speech-02-turbo
TTS_VOICE=Calm_Woman

